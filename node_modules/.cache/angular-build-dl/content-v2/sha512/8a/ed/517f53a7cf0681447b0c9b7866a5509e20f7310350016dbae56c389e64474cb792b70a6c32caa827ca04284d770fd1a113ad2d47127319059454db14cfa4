                component: _modal_loading_loading_page__WEBPACK_IMPORTED_MODULE_8__["LoadingPage"],
                cssClass: 'loadingModal',
                componentProps: {
                    modalCtrl: this.modalController,
                    timeForDismiss: 1000
                }
            });
            this.showSpinner = false;
            return yield modal.present();
        });
    }
    ngOnInit() {
        if (this.showSpinner === true) {
            // this.initialSpinner();
            this.loadingModal();
        }
    }
};
